# Local directories for bind mounts
LOCAL_BASE=/home/circleci
LOCAL_SECRETS=/home/circleci/secrets
LOCAL_LOG=/home/circleci

INTERACTIVE=no

# Container variables defined SWEET Dockerfile ENV commands. 
CONFIG_DIR=/tmp/etc
CONFIG_VOL=/run/etc
DATA_DIR=/var/data
LOGS_DIR=/var/logs
SECRETS_DIR=/tmp/private/secrets
SECRETS_VOL=/run/secrets
SWEETUSER=sweetuser
SWEETUSERID=900
SWEETGROUP=sweetuser
SWEETGROUPID=900

SFTPUSER=sftp
SFTPUSERID=901
SFTPGROUP=sftp
SFTPGROUPID=901






